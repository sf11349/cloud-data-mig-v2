				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "AzureSqlSource",
						"sqlReaderStoredProcedureName": "[adf].[getTableList]",
						"storedProcedureParameters": {
							"SchemaName": {
								"type": "String",
								"value": "staging"
							}
						},
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"dataset": {
						"referenceName": "ds_db_mig_query",
						"type": "DatasetReference"
					},
